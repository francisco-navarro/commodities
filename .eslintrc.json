{
    
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-cond-assign": 2,
        //"no-console": 2,
        "no-constant-condition": 2,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": [1, "all"],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace":  2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 2,
        "valid-typeof": 2,
        "accessor-pairs": [1, { "getWithoutSet": true }],
        "array-callback-return": 1,
        "block-scoped-var": 1,
        "complexity": 1,
        "consistent-return": 1,
        "curly": 2,
        "default-case": 1,
        "dot-location": [2, "property"],
        "dot-notation": 1,
        "eqeqeq": 2,
        "guard-for-in": 1,
        "no-alert": 2,
        "no-caller": 2,
        "no-case-declarations": 2,
        "no-div-regex": 2,
        "no-else-return": 1,
        "no-empty-function": 2,
        "no-empty-pattern": 2,
        "no-eval": 1,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-label": 1,
        "no-fallthrough": 1,
        "no-floating-decimal": 2,
        "no-implicit-coercion": [1, { "string": false }],
        "no-implicit-globals": 1,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 1,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-magic-numbers": [1, { "ignore": [-1, 1, 10, 100, 0, 3000, 3001] } ],
        "no-multi-spaces": [1, { "exceptions": {
        "VariableDeclarator": true,
        "ImportDeclaration": true
        }}],
        "no-multi-str": 2,
        "no-new": 1,
        "no-new-func": 2,
        "no-native-reassign": 2,
        "no-new-wrappers": 2,
        "no-octal-escape": 1,
        "no-param-reassign": [0 , { "props": true }],
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 1,
        "no-self-assign": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unmodified-loop-condition": 2,
        "no-unused-expressions": 1,
        "no-unused-labels": 1,
        "no-useless-call": 1,
        "no-useless-concat": 2,
        "no-useless-escape": 2,
        "no-warning-comments": 1,
        "no-void": 2,
        "no-with": 2,
        "radix": 2,
        "wrap-iife": [2, "inside"],
        "yoda": 2,
        //Strict Mode
        "strict": [2, "function"],
        //Variables
        "no-catch-shadow": 1,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow": [2, {
        "builtinGlobals": true,
        "hoist": "all"
        }],
        "no-shadow-restricted-names": 2,
        "no-undef-init": 1,
        "no-undefined": 2,
        "no-unused-vars": [2, {
        "args": "after-used",
        "caughtErrors": "none"
        }],
        "no-use-before-define": [2, "nofunc"],
        //Node.js and CommonJS*
        //Code style
        "array-bracket-spacing": [1, "never"],
        "block-spacing": 1,
        "brace-style": [1, "1tbs", { "allowSingleLine": true }],
        "camelcase": 2,
        "comma-dangle": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "computed-property-spacing": 2,
        "indent": [1, 2],
        "key-spacing": 1,
        "keyword-spacing": [1, {
        "before": true,
        "overrides": {
            "if": { "before": true },
            "else": { "before": true },
            "for": { "after": false },
            "while": { "after": false }
        }
        }],
        "max-len": [2, 80],
        "max-lines": [1, {"max": 1200, "skipComments": true}],
        "max-nested-callbacks": [2, 6],
        "max-params": [2, 6],
        "max-statements-per-line": [2, { "max": 1 }],
        "newline-after-var": [1, "always"],
        "newline-before-return": 2,
        "newline-per-chained-call": [2, { "ignoreChainWithDepth": 3 }],
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-inline-comments": 2,
        "no-lonely-if": 2,
        "no-mixed-operators": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": [2, {
        "max": 2
        }],
        "no-negated-condition": 2,
        "no-new-object": 2,
        "no-spaced-func": 1,
        "no-trailing-spaces": 2,
        "no-unneeded-ternary": 1,
        "no-whitespace-before-property": 1,
        "object-curly-newline": [ 0, { "minProperties": 2 }],
        "object-curly-spacing": [1, "always"],
        "object-property-newline": 1,
        "one-var-declaration-per-line": [1, "initializations"],
        "operator-assignment": 1,
        "operator-linebreak": 1,
        "padded-blocks": [2, "never"],
        "quote-props": [0, "as-needed", { "keywords": true }],
        "quotes": [2, "single"],
        "require-jsdoc": 0,
        "semi-spacing": 2,
        "semi": 2,
        "sort-vars": 0,
        "space-before-function-paren": [1, "never"],
        "space-in-parens": [1, "never"],
        "space-infix-ops": 1
    }
}